<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:background="@drawable/wodebeijing"
    tools:context="androidlabs.hzuapps.ezu.soft1707080714328.Soft1707080714328Activity_XiaoXi">

    <LinearLayout
        android:orientation="horizontal"
        android:background="#ffffff"
        android:layout_gravity="bottom"
        android:layout_width="match_parent"
        android:layout_height="40dp">


        <ImageButton
            android:id="@+id/wenti"
            android:padding="5dp"
            android:src="@drawable/wenti_0"
            android:scaleType="fitCenter"
            android:background="#00000000"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
        <ImageButton
            android:src="@drawable/gushi_0"
            android:id="@+id/gushi"
            android:padding="5dp"
            android:scaleType="fitCenter"
            android:background="#00000000"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
        <ImageButton
            android:src="@drawable/zixuan_0"
            android:id="@+id/zixuan"
            android:padding="5dp"
            android:scaleType="fitCenter"
            android:background="#00000000"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
        <ImageButton
            android:src="@drawable/xiaoxi_1"
            android:id="@+id/xiaoxi"
            android:padding="5dp"
            android:scaleType="fitCenter"
            android:background="#00000000"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
        <ImageButton
            android:src="@drawable/wo_0"
            android:id="@+id/wo"
            android:padding="5dp"
            android:scaleType="fitCenter"
            android:background="#00000000"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />


    </LinearLayout>
</LinearLayout>
